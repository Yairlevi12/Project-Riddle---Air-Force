generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Operation {
  id             Int      @id @default(autoincrement())
  friendlyLat    Float
  friendlyLng    Float
  threatLat      Float
  threatLng      Float
  speed          Float
  radius         Float
  timestamp      DateTime @default(now())
}